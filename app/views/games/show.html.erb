<%= render "shared/flash_message" %>
<%= render partial: 'card', locals: {game: @game} %>


<% if @game.has_developer?(current_user) %>
    <%= button_to "Edit Game", edit_developer_game_path(@game), :method => :get %>
    <%= form_tag(developer_game_path, :method => :delete) do %>
        <%= submit_tag "Delete" %>
    <% end %>
<% end %>
